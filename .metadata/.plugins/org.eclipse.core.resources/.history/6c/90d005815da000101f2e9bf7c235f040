package vn.iotstar;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.context.annotation.ComponentScan;
import org.springframework.context.annotation.FilterType;

@SpringBootApplication

@ComponentScan(excludeFilters = @ComponentScan.Filter(type = FilterType.ASSIGNABLE_TYPE, classes = vn.iotstar.Demo1.Config.SecurityConfig.class))
public class DemoSpringSecurityJwtApplication {

	public static void main(String[] args) {
		SpringApplication.run(DemoSpringSecurityJwtApplication.class, args);
	}

}
